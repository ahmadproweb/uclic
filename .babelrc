{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": [
              "last 2 Chrome versions",
              "last 2 Firefox versions", 
              "last 2 Safari versions",
              "last 2 Edge versions",
              "last 2 ChromeAndroid versions",
              "last 2 FirefoxAndroid versions",
              "last 2 iOS versions"
            ]
          },
          "modules": false,
          "useBuiltIns": "usage",
          "corejs": 3
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}
